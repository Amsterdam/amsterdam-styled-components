  "author": "Datapunt Amsterdam",
  "homepage": "https://github.com/Amsterdam/amsterdam-styled-components/tree/master/packages/asc-assets#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Amsterdam/amsterdam-styled-components.git"
  },
  "bugs": {
    "url": "https://github.com/Amsterdam/amsterdam-styled-components/issues"
  },
  "version": "0.9.0-alpha.0",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "prebuild": "mkdir -p lib && cp -R src/* lib/ && svgo --config=.svgo.yml -f ./src/assets/Icons/ -o ./lib/Icons",
    "build": "yarn generate",
    "generate": "svgr -d src/Icons src/assets/Icons --template src/utils/generateIconReact.js --svg-props focusable={false} --ext tsx"
  },
  "dependencies": {
    "@svgr/cli": "^4.3.1",
    "@svgr/core": "^2.4.1",
    "svgo": "^1.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "e9fa02de2db7fdc9c60285319df52042d6b217fc"
}
