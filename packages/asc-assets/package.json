  "version": "0.35.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es",
    "lib",
    "static"
  ],
  "scripts": {
    "build": "npm run build:optimize-svg && npm run build:react-icons",
    "build:optimize-svg": "svgo -f static/icons",
    "build:react-icons": "svgr -d src/icons static/icons --template src/utils/generateIconReact.js --svg-props focusable={false} --ext tsx"
  },
  "sideEffects": false,
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "@svgr/core": "^6.1.2",
    "svgo": "^2.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
