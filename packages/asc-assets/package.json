{
  "name": "@datapunt/asc-assets",
  "description": "Contains icons and fonts",
  "license": "MPL-2.0",
  "author": "Datapunt Amsterdam",
  "homepage": "https://github.com/Amsterdam/amsterdam-styled-components/tree/master/packages/asc-assets#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Amsterdam/amsterdam-styled-components.git"
  },
  "bugs": {
    "url": "https://github.com/Amsterdam/amsterdam-styled-components/issues"
  },
  "version": "0.10.0",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "prebuild": "mkdir -p lib && cp -R src/assets lib/",
    "build": "yarn build:optimize-svg && yarn build:react-icons",
    "build:optimize-svg": "svgo --config=.svgo.yml -f ./src/assets/Icons/ -o ./src/tmp/Icons && mkdir -p lib && cp -Rf src/assets/Icons lib/assets",
    "build:react-icons": "svgr -d src/Icons src/tmp/Icons --template src/utils/generateIconReact.js --svg-props focusable={false} --ext tsx && rm -rf ./src/tmp"
  },
  "dependencies": {
    "@svgr/cli": "^4.3.1",
    "@svgr/core": "^2.4.1",
    "svgo": "^1.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "79ff2022d76a557b0f047fcbe64972cc0963e275"
}
